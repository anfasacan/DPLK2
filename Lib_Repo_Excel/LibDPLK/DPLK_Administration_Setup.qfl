
REM ==== Administration Setup Role
Sub Tambah_Setup_Administration_Setup_Role
	Set objkey = CreateObject("WScript.Shell")
	Browser("DPLK").Page("Setup Role").WebButton("Btn Tambah").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role"),"Masuk Menu Tambah Data ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	
	Browser("DPLK").Page("Setup Role").WebEdit("Field Nama Role").Set DataTable.Value("NAMA_ROLE",dtlocalsheet)
	Browser("DPLK").Page("Setup Role").WebElement("Dropdown Grub Role").Click
	Browser("DPLK").Page("Setup Role").WebEdit("Field Search Dropdown").Click
	objkey.SendKeys (DataTable.value("GRUB_ROLE",dtlocalsheet))
	objkey.SendKeys "{ENTER}"
	Browser("DPLK").Page("Setup Role").WebElement("Dropdown Status Aktif").Click
	Browser("DPLK").Page("Setup Role").WebEdit("Field Search Dropdown").Click
	objkey.SendKeys (DataTable.value("STATUS_AKTIF",dtlocalsheet))
	objkey.SendKeys "{ENTER}"
	
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role"),"Isi Field Yang Diperlukan Dan Tekan Simpan ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Setup Role").WebButton("Btn Simpan").Click
	wait 7
	
	If Browser("DPLK").Page("Setup Role").WebElement("Notifikasi").Exist(2) Then
		TextNotif = Browser("DPLK").Page("Setup Role").WebElement("Notifikasi").GetROProperty("innertext")
		TextNotifBerhasil = Instr(ucase(TextNotif),"BERHASIL")
	End If
	
	
	If TextNotifBerhasil <> 0 Then
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role"),mid(TextNotif,2,len(TextNotif)-1), " ",  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
		Browser("DPLK").Page("Setup Role").WebButton("Btn X Notifikasi").Click
	End If
	
End Sub

