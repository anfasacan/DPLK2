REM ==== Administration Setup Role User

Sub Ubah_Setup_Administration_Setup_Role_User
	Set objkey = CreateObject("WScript.Shell")
	Browser("DPLK").Page("Setup Role User").WebEdit("Field Search").Set DataTable.Value("USERNAME",dtlocalsheet)
	wait 5
	Browser("DPLK").Page("Setup Role User").WebElement("Btn Ubah").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role User"),"Masuk Menu Ubah Data ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	
	Jumlah_Row = Browser("DPLK").Page("Setup Role User").WebTable("Tabel Kode Role").RowCount()
	For Iterator = 1 To Jumlah_Row Step 1
		kode_role = Browser("DPLK").Page("Setup Role User").WebTable("Tabel Kode Role").GetCellData(iterator,2)
		If ucase(kode_role) = ucase(DataTable.Value("KODE_ROLE",dtlocalsheet)) Then
			set CekBox = Browser("DPLK").Page("Setup Role User").WebTable("Tabel Kode Role").ChildItem(iterator,1,"WebCheckBox",0)
			CekBox.click
			Exit for
		End If
	Next
	
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role User"),"Ubah Field Yang Diperlukan Dan Tekan Simpan ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Setup Role User").WebButton("Btn Simpan").Click
	wait 7
		If Browser("DPLK").Page("Setup Role User").WebElement("Notifikasi").Exist(2) Then
		TextNotif = Browser("DPLK").Page("Setup Role User").WebElement("Notifikasi").GetROProperty("innertext")
		TextNotifBerhasil = Instr(ucase(TextNotif),"BERHASIL")
	End If
	
	
	If TextNotifBerhasil <> 0 Then
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Role User"),mid(TextNotif,2,len(TextNotif)-1), " ",  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
		Browser("DPLK").Page("Setup Role User").WebButton("Btn X Notifikasi").Click
